{
  "ts-node": {
    "compilerOptions": {
      "target": "ES2020",
      "module": "nodenext"
    }
  },
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "target": "ES2020",
    "module": "NodeNext",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}